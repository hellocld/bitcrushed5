        vertex(width, height, 
                (width  + scrollX * background.width) * scaleX, (height + scrollY * background.height) * scaleY);
        
        vertex(0, height, 
                (scrollX * background.width) * scaleX, (height + scrollY * background.height) * scaleY);
        
        
        endShape();
    \}
